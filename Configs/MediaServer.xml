<?xml version="1.0"?>
<AutomationConfig>

	<SourceConfigs>
		<Config id="Plex" Load="True" ConfigFile="Configs-Plex.xml" />
		<Config id="Radarr" Load="True" ConfigFile="Configs-Radarr.xml" />
		<Config id="Sonarr" Load="True" ConfigFile="Configs-Sonarr.xml" />
		<Config id="Gmail" Load="True" ConfigFile="Configs-Gmail.xml" />
		<Config id="Tautulli" Load="True" ConfigFile="Configs-Tautulli.xml" />
	</SourceConfigs>

	<DropboxConfigs>
		<DropBoxSizeCheck>False</DropBoxSizeCheck>
		<DropBoxSizeLimit>1,000,000,000</DropBoxSizeLimit>
		<DropboxPath>$env:userprofile\Dropbox\Camera Uploads</DropboxPath>

		<EnableDropboxBackup>False</EnableDropboxBackup>
		<DropboxBCKDest>\\SERVER\SharedProfiles\Dropbox</DropboxBCKDest>
	</DropboxConfigs>

	<TVRenameCheck>False</TVRenameCheck>
	<TVRenamePath>C:\Program Files (x86)\TVRename</TVRenamePath>

	<ZipPath>C:\Program Files\7-Zip</ZipPath>
	<ZipArchivePwd>Random</ZipArchivePwd>

	<SeedBoxCheckForRar>True</SeedBoxCheckForRar>
	<SeedBoxDownLocation>E:\Data\Downloads\Seedbox</SeedBoxDownLocation>

	<BtSyncCheckForFiles>True</BtSyncCheckForFiles>
	<BtSync_FolderLocation>E:\Data\Downloads\btsync</BtSync_FolderLocation>
	<BtSync_Secret_Key>SDAAADAF6D024DD40B816516BA4A</BtSync_Secret_Key>

	<ServiceCheck>
        <service name="Emby" FriendlyName="Media Streaming Service (Emby)" executable="MediaBrowser.ServerApplication.exe" state="running" />
        <service name="PlexService" FriendlyName="Media Streaming Service (Plex)" executable="PlexService.exe" state="running" />
		<service name="rslsyncsvc" FriendlyName="Transfer Service (Resilio)" executable="Resilio Sync.exe" state="running" />
		<service name="NzbDrone" FriendlyName="TV Organizer Service (Sonarr)" executable="NzbDrone.Console.exe" state="running" />
		<service name="Radarr" FriendlyName="Movie Organizer Service (Radaar)" executable="Radarr.Console.exe" state="running" />
		<service name="Lidarr" FriendlyName="Music Organizer Service (Ladarr)" executable="Lidarr.Console.exe" state="running" />
		<service name="Jackett" FriendlyName="Index Feed Service (Jackett)" executable="JackettService.exe" state="running" />
		<service name="Ombi" FriendlyName="Plex Request Service (ombi)" executable="Ombi.exe" state="running" />
        <service name="Tomcat9" FriendlyName="Web Service (subsonic)" executable="Tomcat9.exe" state="running" />
        <service name="W3SVC" FriendlyName="Web Service (IIS)" executable="svchost.exe" state="running" />
        <service name="Unifi" FriendlyName="UniFi Controller" executable="UniFi.exe" state="running" />
	</ServiceCheck>

	<ProcessCheck>
		<process name="Plex Media Server" exePath="C:\Program Files (x86)\Plex\Plex Media Server\Plex Media Server.exe" running="yes" />
		<process name="peerblock" exePath="C:\Program Files\PeerBlock\peerblock.exe" running="yes" />
		<process name="Azureus" exePath="C:\Program Files\Vuze\Azureus.exe" running="yes" />
		<process name="Tautulli" exePath="C:\Python27\python.exe" command="E:\Data\Plex\Tautulli\Tautulli.py" running="yes" />
		<process name="uTorrent" exePath="C:\Users\PlexAdmin\AppData\Roaming\uTorrent\uTorrent.exe" running="no" />
		<process name="NETGEAR Genie" exePath="C:\Program Files (x86)\NETGEAR Genie\bin\NETGEARGenie.exe" running="no" />
		<process name="Dropbox" exePath="C:\Program Files (x86)\Dropbox\Client\Dropbox.exe" running="no" />
		<process name="Tomcat" exePath="C:\Program Files (x86)\Apache Software Foundation\Tomcat 7.0\bin\Tomcat7.exe" running="yes" />
	</ProcessCheck>

	<DirectoryConfigs>
		<ProcessDownloads>
			<Folder>D:\Data\Downloads\btsync</Folder>
			<Folder>D:\Data\Downloads\Seedbox</Folder>
			<Folder>D:\Data\Downloads\Torrents\Completed</Folder>
			<Folder>D:\Data\Downloads\Torrents\Completed</Folder>
		</ProcessDownloads>

		<KeepDeletedDataDays>5</KeepDeletedDataDays>

		<RSSTorrentCheck>True</RSSTorrentCheck>

		<DeleteTorrents>D:\Data\Downloads\Torrents\Deleted</DeleteTorrents><!-- If blank, content will delete permanently-->
		<ProcessedTorrents>E:\Data\Downloads\Torrents\Processed</ProcessedTorrents>
		<InvalidTorrents>E:\Data\Downloads\Torrents\Failed</InvalidTorrents>

	</DirectoryConfigs>

</AutomationConfig>